{
  "name": "ember-inline-svg",
  "description": "Ember CLI addon to render SVG images inline",
  "version": "1.0.0",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test:all": "ember try:testall",
    "test:default": "ember try:one ember-lts-2.16 --- ember test"
  },
  "repository": "https://github.com/harrisjose/ember-inline-svg",
  "engines": {
    "node": ">= 6"
  },
  "author": {
    "name": "Harris Jose",
    "email": "harrisjose@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-cli": "~3.0.0",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "^0.4.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-release": "^0.2.9",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-code-snippet": "^2.0.1",
    "ember-disable-prototype-extensions": "^1.1.0",
